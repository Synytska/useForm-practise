generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}
enum Category {
  work
  home
  other
}


model Person {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  firstname    String
  lastname     String
  email        String    @unique
  phonenumber  String    @unique
  category     Category
}